  "uprn": "100023403690",
  "decision_date": "06/11/2019",
  "description": "Erection of three additional floors to provide an additional  36 flats.",
  "site_name": "Westward House",
  "last_updated_by": "Northgate M3",
  "decision_target_date": "26/10/2017",
  "borough": "Hounslow",
